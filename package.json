{
  "name": "overrustle",
  "version": "1.0.0",
  "description": "OverRustle",
  "private": true,
  "scripts": {
    "start": "node ./lib/server.js",
    "clean": "rm -rf lib public/assets public/index.html",
    "build": "npm run clean && npm run build:babel && npm run build:bundle",
    "build:babel": "babel src --out-dir lib",
    "build:bundle": "webpack --progress --display-chunks --display-reasons --display-error-details --display-modules",
    "build:production": "NODE_ENV=production npm run build",
    "dev-server": "npm run build &&  webpack-dev-server --inline --hot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint *.js src/"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "cookies": "^0.6.2",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draggable": "^2.2.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "recompose": "^0.22.0",
    "reconnecting-websocket": "^3.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.29.0",
    "sqlite3": "^3.1.8",
    "string-hash": "^1.1.1",
    "uuid": "^3.0.1",
    "uws": "^0.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.2.0",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-subresource-integrity": "^0.8.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "development": {
        "plugins": []
      },
      "production": {
        "plugins": []
      }
    }
  }
}
